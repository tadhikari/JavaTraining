package Assignment8;

public class Class1 {
	
	int i;
	int j;
	boolean k;
	boolean l;
	
	public Class1() {
	
	
	}
	
	
	public static void main(String args[]) {
		i = 22;
		j = 14000;
		
		Class1 class1 = new Class1();
		k = class1.method1(i);
		
		
		
		
		
	}
	
	public boolean method1(int i) {
		
		if(i>20 && i<30) {
			return true;
		}
		else {
			return false;
		}
		
	
	}
	
	public boolean method2(int j) {
		
		if(j>=14000 && j<20000) {
			return true;
		}
		else {
			return false;
		}
		
	}
}
